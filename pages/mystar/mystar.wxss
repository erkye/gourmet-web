/* pages/mypublish/mypublish.wxss */
.search-value-item{
    width: 100%;
    height: 200rpx;
  }
  .search-value-item navigator{
    display: flex;
  }
  .search-value-item navigator image{
    width: 360rpx;
    height: 180rpx;
    padding-left: 30rpx;
    padding-top: 20rpx;
  }
  .item-content{
    padding-left: 30rpx;
  }
  .item-title{
    padding-top: 20rpx;
    font-size: large;
    font-weight: 700;
  }
  .item-introd{
    padding-top: 15rpx;
    font-size: smaller;
  }
  .item-user{
    padding-top: 15rpx;
    font-size: smaller;
  }
  .item-scan text:nth-child(1){
    padding-top: 15rpx;
    font-size: smaller;
    color: #bb2205;
  }
  .item-scan text:nth-child(3){
    padding-top: 15rpx;
    font-size: smaller;
    color: #2d6187;
  }